{
  "name": "gatsby-starter-typescript-deluxe",
  "description": "A Gatsby starter with TypeScript, Storybook, Styled Components, Jest, and more.",
  "version": "0.0.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gojutin/gatsby-starter-typescript-deluxe"
  },
  "bugs": {
    "url": "https://github.com/gojutin/gatsby-starter-typescript-deluxe/issues"
  },
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "lint:watch": "esw -w . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "npm run lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "serve": "gatsby serve",
    "storybook": "start-storybook -p 6006",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "build:storybook": "build-storybook",
    "update": "ncu -u"
  },
  "dependencies": {
    "framer-motion": "^1.6.15",
    "gatsby": "^2.17.9",
    "gatsby-image": "^2.2.30",
    "gatsby-plugin-manifest": "^2.2.26",
    "gatsby-plugin-offline": "^3.0.18",
    "gatsby-plugin-react-axe": "^0.2.2",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-sharp": "^2.2.36",
    "gatsby-plugin-styled-components": "^3.1.11",
    "gatsby-plugin-typescript": "^2.1.15",
    "gatsby-source-filesystem": "^2.1.35",
    "gatsby-transformer-sharp": "^2.3.2",
    "modern-css-reset": "^1.0.4",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.1",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.2",
    "@types/jest": "^24.0.22",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-helmet": "^5.0.14",
    "@types/styled-components": "^4.1.21",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/eslint-plugin-tslint": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-gatsby": "^0.2.20",
    "eslint": "^6.6.0",
    "eslint-config-gojutin": "0.0.16",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "eslint-watch": "^6.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.3",
    "npm-check-updates": "^3.2.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-immutable": "^6.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.2"
  }
}
